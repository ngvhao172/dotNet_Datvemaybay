<div class="body">
        <div class="container-fluid p-5">
            <div class="row">
                <div class="col-2">
                    <div class="list-group" id="list-tab" role="tablist">
                        <button id="statistic" class="list-group-item list-group-item-action active"
                            id="list-settings-list" data-bs-toggle="list" href="#list-settings" role="tab"
                            aria-controls="list-settings">Thống
                            kê thu nhập</button>
                    </div>
                </div>
                <div class="col-10">
                    <div class="tab-content" id="nav-tabContent">
                        <div class="tab-pane fade show active" id="list-settings" role="tabpanel"
                            aria-labelledby="list-settings-list">
                            <div class="container">
                                <div class="row mb-3">
                                    <div class="col-4">
                                        <div class="input-group date datetimepicker" data-target-input="nearest">
                                            <div class="input-group-text d-block">Ngày đi <i class="fa fa-calendar"
                                                    style="height: 100%;"></i></div><input required id="search_fromdate"
                                                type="date" class="form-control datetimepicker-input"
                                                data-target=".datetimepicker" />
                                            <div class="input-group-append" data-target="#datetimepicker"
                                                data-toggle="datetimepicker">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-4">
                                        <div class="input-group date datetimepicker" data-target-input="nearest">
                                            <div class="input-group-text d-block">Ngày về <i class="fa fa-calendar"
                                                    style="height: 100%;"></i></div><input required id="search_todate"
                                                type="date" class="form-control datetimepicker-input"
                                                data-target=".datetimepicker" />
                                            <div class="input-group-append" data-target="#datetimepicker"
                                                data-toggle="datetimepicker">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-4">
                                        <input type="button" name="save" id="btn_search" class="btn btn-info"
                                            value="Thống kê" />
                                    </div>
                                </div>
                                <div class="tab-content" id="nav-tabContent">
                                    <div class="tab-pane fade show active" id="list-messages" role="tabpanel"
                                        aria-labelledby="list-messages-list">
                                        <table class="table table-striped" id="dt-statistic" style="width: 100%">
                                            <thead>
                                                <tr>
                                                    <th scope="col">TỔNG SỐ LƯỢNG VÉ</th>
                                                    <th scope="col">DOANH THU</th>
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>